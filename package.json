{
  "name": "node-bin-gen",
  "version": "7.1.4",
  "description": "Generate a node binary package",
  "author": "Aria Stewart <aredridel@dinhe.net>",
  "main": "index.js",
  "bin": "index.js",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/aredridel/node-bin-gen"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "make-fetch-happen": "^2.6.0",
    "pump": "^2.0.0",
    "rimraf": "^2.6.2",
    "verror": "^1.9.0",
    "yargs": "^10.0.3"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6
    },
    "rules": {
      "valid-typeof": "error",
      "no-undef": "error",
      "no-unused-vars": "error",
      "no-process-exit": 0,
      "no-shadow": 0,
      "strict": 0,
      "eqeqeq": 0,
      "curly": 0
    },
    "env": {
      "node": true,
      "es6": true
    }
  }
}
